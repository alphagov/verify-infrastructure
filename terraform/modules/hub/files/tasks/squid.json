[
  {
    "name": "squid",
    "image": "${image_identifier}",
    "cpu": 1024,
    "memory": 3500,
    "essential": true,
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "environment": [{
      "Name": "WHITELIST",
      "Value": "${whitelist_base64}"
    }],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${deployment}-hub",
        "awslogs-region": "${region}",
        "awslogs-stream-prefix": "squid",
        "awslogs-create-group": "true"
      }
    }
  }
]
